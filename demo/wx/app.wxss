/**app.wxss**/
@import './weui.wxss';
page {
  font-size: 16px;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}
.page__hd {
  padding: 40px;
}
.page__bd {
  padding-bottom: 40px;
}
.page__bd_spacing {
  padding-left: 15px;
  padding-right: 15px;
}
.page__title {
  text-align: left;
  font-size: 20px;
  font-weight: 400;
}
.page__desc {
  margin-top: 5px;
  color: #888;
  text-align: left;
  font-size: 14px;
}
code {
  background-color: #f0f0f0;
  font-size: 85%;
  margin-right: 3px;
  padding: 2px 5px 2px 5px;
  border-radius: 2px;
  font-family: SFMono-Regular,Consolas,Liberation Mono,Menlo,monospace;
}